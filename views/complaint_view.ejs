          <div class="container-fluid mt-4 mb-4">
            <div class="container-fluid mt-3 border_t_g newmp bg-light py-2 rounded-3">
              <ul class="timeline">
                <% if (data1[0].ticket_Created_on && data1[0].raised_by) { %>
                  <li>
                    <div class="direction-r">
                      <div class="flag-wrapper">
                        <span class="hexa"></span>
                        <span class="flag f_14 fw_600">Opened</span>
                        <span class="time-wrapper"><span class="time time f_13 fw_600"><i class="fa-solid fa-clock"></i> <%= data1[0].ticket_Created_on %></span></span>
                      </div>
                      <div class="desc f_13 fw_600">Created by: <%= data1[0].raised_by %></div>
                    </div>
                  </li>
                <% } else { %>
                  <li>
                    <div class="direction-r">
                      <div class="flag-wrapper">
                        <span class="hexa"></span>
                        <span class="flag f_14 fw_600">No data found</span>
                      </div>
                    </div>
                  </li>
                <% } %>
          
                <!-- In-process -->
                <% if (data1[0].formated_inprocess_at && data1[0].inprocess_by) { %>
                  <li>
                    <div class="direction-l">
                      <div class="flag-wrapper">
                        <span class="hexa"></span>
                        <span class="flag f_14 fw_600">In-process</span>
                        <span class="time-wrapper"><span class="time f_13 fw_600"><i class="fa-solid fa-clock"></i> <%= data1[0].formated_inprocess_at %></span></span>
                      </div>
                      <div class="desc f_13 fw_600">Processed by: <%= data1[0].inprocess_by %></div>
                    </div>
                  </li>
                <% } else { %>
                  <li>
                    <div class="direction-l">
                      <div class="flag-wrapper">
                        <span class="hexa"></span>
                        <span class="flag f_14 fw_600">No data found</span>
                      </div>
                    </div>
                  </li>
                <% } %>
                <!-- Resolved -->
                <% if (data1[0].formated_resolved_at && data1[0].resolved_by) { %>
                  <li>
                    <div class="direction-r">
                      <div class="flag-wrapper">
                        <span class="hexa"></span>
                        <span class="flag f_14 fw_600">Resolved</span>
                        <span class="time-wrapper"><span class="time f_13 fw_600"><i class="fa-solid fa-clock"></i> <%= data1[0].formated_resolved_at %></span></span>
                      </div>
                      <div class="desc f_13 fw_600">Resolved by: <%= data1[0].resolved_by %></div>
                    </div>
                  </li>
                <% } else { %>
                  <li>
                    <div class="direction-r">
                      <div class="flag-wrapper">
                        <span class="hexa"></span>
                        <span class="flag f_14 fw_600">No data found</span>
                      </div>
                    </div>
                  </li>
                <% } %>
          
                <!-- Closed -->
                <% if (data1[0].formated_closed_at && data1[0].closed_by) { %>
                  <li>
                    <div class="direction-l">
                      <div class="flag-wrapper">
                        <span class="hexa"></span>
                        <span class="flag f_14 fw_600">Closed</span>
                        <span class="time-wrapper"><span class="time f_13 fw_600"><i class="fa-solid fa-clock"></i> <%= data1[0].formated_closed_at %></span></span>
                      </div>
                      <div class="desc f_13 fw_600">Closed by: <%= data1[0].closed_by %></div>
                    </div>
                  </li>
                <% } else { %>
                  <li>
                    <div class="direction-l">
                      <div class="flag-wrapper">
                        <span class="hexa"></span>
                        <span class="flag f_14 fw_600">No data found</span>
                      </div>
                    </div>
                  </li>
                <% } %>
          
